{
  "name": "find-my",
  "author": "Alex Wolfe",
  "version": "1.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "message-queue-pebble": "^1.0.0",
    "pebble-clay": "^1.0.4"
  },
  "devDependencies": {
    "husky": "^7.0.4"
  },
  "pebble": {
    "displayName": "find-my",
    "uuid": "a24f569b-658a-43bd-8935-821a9e14e6ed",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "BatteryLevel",
      "BatteryStatus",
      "Command",
      "DeviceClass",
      "DeviceName",
      "DeviceId",
      "DeviceStatus",
      "Status",
      "Server",
      "Username",
      "Password"
    ],
    "resources": {
      "media": [
        {
          "type": "raw",
          "name": "CONFIRMATION",
          "file": "images/Pebble_25x25_Generic_confirmation.pdc"
        },
        {
          "type": "raw",
          "name": "FAILED",
          "file": "images/Pebble_25x25_Generic_failed.pdc"
        }
      ]
    }
  },
  "scripts": {
    "prepare": "cd .. && husky install"
  }
}
